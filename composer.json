{
    "name": "homeleon/framework",
    "description": "Homeleon framework",
    "keywords": ["framework", "Homeleon"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pukhniuk Viktor",
            "email": "wirus@ukr.net"
        }
